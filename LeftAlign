package Algorithms.JavaFundamentalApi.Print;

public class LeftAlign {
    public static void main(String[] args) {
        int[][] example = {{1, -1, 2}, {3, 11, 20}, {21, -18, 0}};
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                System.out.printf("%-5d", example[i][j]);
            }
            System.out.println();
        }
    }
}
